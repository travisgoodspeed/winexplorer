Function GetGuideByte()
    Dim RetVal
    Dim RetVal2
    Dim PromptString
    Dim GotInput


    Do

    PromptString = "Please refer to the (Local Bytes.txt) to find the best guide byte for locals." & vbcr & vbcr & "49 - Normal Guide Byte (No Locals)" & vbcr & "85 - Normal Guide Byte (With Locals)" & vbcr & "FF - Engineering Guide Byte"
    RetVal = Sc.InputBox(PromptString, "Please Enter Guide Byte")

    GotInput = False

    If RetVal = "" then
        Gotinput = True
        GetGuideByte = "-1"
    else
        if len(RetVal) <> 2 then
            GotInput = false
        else
          RetVal = UCase(RetVal)
          GetGuideByte = RetVal
          GotInput = True
        End If
    end if

    Loop until GotInput = True

end function


Function HexToDec(HexNumber)
    ' This function takes a string as input, assuming it to be a Hexidecimal string,
    ' and converts it to a decimal number.
    HexNumber=Replace(UCase(HexNumber)," ","")
    HexToDec=CLng("&H"+HexNumber)
End Function