Sub Main()
    ' Main Program Loop and Menu
    Dim MenuChoice
    Dim MsgPrompt
    Dim ErrorValue

    ' Suppress window output
    Sc.Verbose=False

    ' Verify correct version of WinExplorer
    If VerifyWinExplorer()=0 Then Exit Sub

    ' Verify that an unlooper is connected and we can communicate with it
    'If VerifyUnlooper()=0 Then Exit Sub
     Call VerifyUnlooper

    'Reset second function variable
    func2=0

    Do
        MsgPrompt="=== "+ScriptName+" "+ScriptVer+" ===>" & "  By: Hitec" +vbCr+vbCr
        'MenuChoice=Sc.ButtonBox(MsgPrompt,vbDefaultButton1,ScriptName+" "+ScriptVer+" - Main Menu","Read Card","Write .img/.bin","Unloop Card", "More..","Exit")
        MenuChoice=Sc.ButtonBox(MsgPrompt & "Read Card - Read a card and save it as .bin file." & vbcr & "Write Card - Write a .bin file to a card." & vbcr & "Unloop Card - Unloop a looped card." & vbcr & "More.. - Next Page of buttons." & vbcr & "Exit - Exit Loader H",vbDefaultButton1,ScriptName+" "+ScriptVer+" - Main Menu","Read Card","Write .img/.bin","Unloop Card", "Next Page...","Exit")

        ' Initialize Glitch values
        Call SetGlitchValues()

        Select Case MenuChoice
            Case 1:
                ErrorValue=ReadCard()
            Case 2:
                ErrorValue=WriteCard()
            Case 3:
                ErrorValue=FixCard()
            Case 4:
                ErrorValue=More()
        End Select
    Loop Until MenuChoice=5

End Sub


